<Window x:Name="mainWind" x:Class="HRSH_Transpera.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HRSH_Transpera"
        mc:Ignorable="d"
        Title="HRSH Transpera" Height="450" Width="800" ResizeMode="NoResize" Loaded="mainWind_Loaded">
    <Window.Background>
        <ImageBrush ImageSource="/img/570413.jpg"/>
    </Window.Background>
    <Grid HorizontalAlignment="Left" Width="800">
        <Rectangle HorizontalAlignment="Left" Width="234" Height="NaN" Fill="#3F000000"/>
        <StackPanel x:Name="modsList" Margin="0,40,566,0"/>
        <Label Content="Your Modifications" HorizontalAlignment="Left" Margin="10,6,0,0" VerticalAlignment="Top" Foreground="White" FontSize="16"/>
        <Image x:Name="btnAddMod" HorizontalAlignment="Left" Height="33" Margin="200,5,0,0" VerticalAlignment="Top" Width="33" Source="/img/icons8-add-new-90.png" MouseDown="btnAddMod_MouseDown"/>
    </Grid>
</Window>
